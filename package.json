{
  "name": "obs-projects-plus",
  "version": "3.0.0",
  "description": "Powerful project management for Obsidian with Apple-style calendar, timeline view, zoom gestures, and infinite scroll",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "build:analyze": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && npx esbuild-bundle-analyzer main.js",
    "bundle:stats": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production --analyze",
    "bundle:size": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && Get-ChildItem main.js | Select-Object Name, Length",
    "version": "node scripts/version-bump.mjs && git add manifest.json versions.json",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:unit": "jest --config jest.config.js --testPathPattern=unit",
    "test:integration": "jest --config jest.config.js --testPathPattern=integration",
    "test:performance": "jest --config jest.config.js --testPathPattern=performance",
    "svelte-check": "svelte-check",
    "lint": "eslint ./src",
    "format": "prettier -w ./src",
    "quality:check": "npm run test:coverage && npm run build && npm run lint",
    "benchmark": "jest --config jest.config.js --testPathPattern=performance --verbose"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "project-management",
    "productivity"
  ],
  "author": "Park Pavel <https://parkpavel.github.io/park-pavel/>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/ParkPavel/obs-projects-plus",
  "repository": {
    "type": "git",
    "url": "https://github.com/ParkPavel/obs-projects-plus.git"
  },
  "bugs": {
    "url": "https://github.com/ParkPavel/obs-projects-plus/issues"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@popperjs/core": "^2.11.8",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^4.2.3",
    "@tsconfig/svelte": "^5.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "builtin-modules": "^3.3.0",
    "dayjs": "^1.11.11",
    "esbuild": "^0.24.0",
    "esbuild-bundle-analyzer": "^0.0.1",
    "esbuild-jest": "^0.5.0",
    "esbuild-plugin-replace": "^1.4.0",
    "esbuild-svelte": "^0.7.4",
    "eslint": "^9.15.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "fp-ts": "^2.16.6",
    "globals": "^15.12.0",
    "i18next": "^24.0.2",
    "immer": "^10.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "obsidian": "^1.7.2",
    "obsidian-dataview": "^0.5.67",
    "obsidian-svelte": "0.2.1",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.3.0",
    "sass": "^1.77.2",
    "svelte": "^3.59.2",
    "svelte-check": "^3.7.1",
    "svelte-dnd-action": "0.9.52",
    "svelte-i18next": "^2.2.2",
    "svelte-jest": "^0.3.1",
    "svelte-media-queries": "^1.6.2",
    "svelte-preprocess": "^5.1.4",
    "ts-essentials": "^10.0.3",
    "ts-jest": "^29.1.3",
    "tslib": "^2.6.2",
    "typescript": "^5.6.3",
    "uuid": "^11.0.3",
    "yaml": "^2.4.2"
  }
}
